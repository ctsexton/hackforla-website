    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-preset-vue": "^2.0.2",
    "bourbon": "^5.0.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
